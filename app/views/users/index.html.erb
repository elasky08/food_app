<script src="//maps.google.com/maps/api/js?v=3.23&key=AIzaSyDY_RtfOh4eOUbooRCQ7owonryU830hn4Q"></script>
<script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>
<script src='//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' type='text/javascript'></script>

<h1>All bakers</h1>
<div class="navbar-form">
<%= form_tag users_path, :method => 'get' do %>
  <%= text_field_tag :search, params[:search], class: 'form-control' %>
  <%= submit_tag "Search", class: 'btn btn-default'%>
<!-- <%# submit_tag "Search", :name => nil %> why is there :name => nil ? I don't know-->
<% end %>
</div>
<div>
  <% @users.each do |u| %>
  <h2><%= link_to u.shop_name, user_path(u) %></h2>
  <p><%= u.shop_name %></p>
  <p><%= u.full_address %></p>
  <p><%= u.first_name %></p>
  <p><%= u.last_name %></p>

  <button id="add-maps" type="submit">show map</button>
  <hr>
  <% end %>
</div>
<%= render "maps", u: u %>
<%= paginate @users %>
